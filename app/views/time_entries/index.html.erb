<h1>Time Entry List</h1>
<%= link_to 'New Time Entry', new_time_entry_path %>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Date</th>
      <th>Project ID</th>
      <th>Developer ID</th>
      <th>Duration</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |t| %>
      <tr>
        <td><%= t.id %></td>
        <td><%= t.date %></td>
        <td><%= t.project.name %></td>
        <td><%= t.developer.name %></td>
        <td><%= t.duration %></td>
        <td><%= link_to 'Show', t %></td>
        <td><%= link_to 'Edit', edit_time_entry_path(t.id) %></td>
        <td><%= link_to 'Destroy', time_entry_path(t.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
